
        <div class="row">
            <!-- STAFF OVERVIEW PAGE -->
            <div class="col-lg-12">
                   <div class="table-responsive">
                        <table class="table table-bordered ">
                            <thead>
                                <tr class="btn-danger">                         
                                    <!--<th>FULL NAME</th>-->
                                    <th width="250px">TITLE</th>
                                    <th>MESSAGE</th>
                                    <th width="250px">DATETIME</th>
                                    <th width="30px">&nbsp;</th>
                                </tr>
                            </thead>
                            <tbody>
                                 <%  if(data[2].length > 0){
                                     
                                        data[2].map((row,i) => {
                                     
                                    %>
                                    <tr style="<%- row.read_flag == '1' ? 'background-color:#f5e5e5' : ''%>">
                                        <td><small><b><%- row.action != null ? (row.action.substring(0,34)).toUpperCase() :''%></b></small></td>
                                        <td><small><em><b class="text-info"><%- row.message != null ? (row.message.substring(0,48)+' ...').toUpperCase() :''%></b></em></small></td>
                                        <td><small><b class="text-warning"><%- row.logtime.toUpperCase() %></b></small></td>
                                        <td>
                                            <div class="group group-xl" style="width:230px">
                                                <a class="btn btn-info btn-sm btn-left" title="Read Notification" href="/hrm/st/notes/full?id=<%- row.id%>"><i class="fa fa-envelope"></i>&nbsp;&nbsp;<small>READ</small></a>                                                                                             
                                            </div>
                                        </td>
                                    </tr>
                                <%    }) 
                                   }else{ %> 
                                <%- '<tr><td colspan="4" align="center"><strong><em class="text-danger">No Notifications for staff!</em></strong></td></tr>' %>
                                <% }%>

                        </table>
                   </div>
            </div>    
        </div>
    